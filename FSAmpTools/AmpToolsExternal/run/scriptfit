      echo GENERATE PHASE SPACE
      echo
      echo --------------------------------
      echo
      #sleep 3
 
cd $FSAMPTOOLS/run
$FSAMPTOOLS/FSAmpToolsExe/generatePhaseSpace phasespace.gen.root 100000
 
 
      #sleep 1
      echo
      echo --------------------------------
      echo
      echo MAKE PHASE SPACE PLOTS
      echo
      echo --------------------------------
      echo
      #sleep 3
 
cd $FSAMPTOOLS/run
$FSAMPTOOLS/FSAmpToolsExe/plotData phasespace.gen.root plots.phasespace.gen.root
 
 
#       #sleep 1
#       echo

        echo
        echo GENERATE PHYSICS
        echo
        echo --------------------------------
        echo
        #sleep 3
   
  cd $FSAMPTOOLS/run
  $FSAMPTOOLS/FSAmpToolsExe/generatePhysics fsamptools1.cfg physics.gen.root 100000
 
 
        #sleep 1
        echo
        echo --------------------------------
        echo
        echo MAKE PHYSICS PLOTS
        echo
        echo --------------------------------
        echo
        #sleep 3
   
  cd $FSAMPTOOLS/run
  $FSAMPTOOLS/FSAmpToolsExe/plotData physics.gen.root plots.physics.gen.root
 
 
#        #sleep 1
#        echo

        echo
        echo SEND PHASE SPACE THROUGH TOY DETECTOR
        echo
        echo --------------------------------
        echo
        #sleep 3
   
  cd $FSAMPTOOLS/run
  $FSAMPTOOLS/FSAmpToolsExe/toyAcceptance phasespace.gen.root phasespace.acc.root
 
 
        #sleep 1
        echo
        echo --------------------------------
        echo
        echo MAKE PHASE SPACE PLOTS -- POST-DETECTOR
        echo
        echo --------------------------------
        echo
        #sleep 3
  
  cd $FSAMPTOOLS/run
   $FSAMPTOOLS/FSAmpToolsExe/plotData phasespace.acc.root plots.phasespace.acc.root
 
 

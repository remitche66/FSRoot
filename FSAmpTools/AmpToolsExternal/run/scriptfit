        echo --------------------------------
        echo
        #sleep 3
  
  cd $FSAMPTOOLS/run
  $FSAMPTOOLS/FSAmpToolsExe/toyAcceptance physics.gen.root physics.acc.root
 
 
        #sleep 1
        echo
        echo --------------------------------
        echo
        echo MAKE PHYSICS PLOTS -- POST-DETECTOR

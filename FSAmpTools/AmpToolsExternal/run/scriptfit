        #sleep 3
   
  cd $FSAMPTOOLS/run
  $FSAMPTOOLS/FSAmpToolsExe/fitAmplitudes fsamptools1.cfg
 
 
        #sleep 1
        echo
        echo --------------------------------
        echo
        echo MAKE PLOTS OF RESULTS
        echo
        echo --------------------------------
        echo
        #sleep 3
   
  cd $FSAMPTOOLS/run
  $FSAMPTOOLS/FSAmpToolsExe/plotResults fsamptools1.fit fsamptools1.root
 
 
        #sleep 1
        echo
        echo --------------------------------
        echo
        echo SHOW PLOTS IN ROOT
        echo
        echo --------------------------------
        echo
        #sleep 3
    
  cd $FSAMPTOOLS/run
  root -l plots.physics.acc.root fsamptools1.root plotResult.C
